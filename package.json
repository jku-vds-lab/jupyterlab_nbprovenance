{
  "name": "@jku-icg/jupyterlab_nbprovenance",
  "version": "1.0.0-alpha.1",
  "description": "JuypterLab extension for provenance tracking in notebooks.",
  "keywords": [
    "jupyter",
    "notebook",
    "jupyterlab",
    "jupyterlab-extension",
    "provenance",
    "reproducability"
  ],
  "homepage": "https://github.com/jku-icg/jupyterlab_nbprovenance/",
  "bugs": {
    "url": "https://github.com/jku-icg/jupyterlab_nbprovenance/issues"
  },
  "license": "BSD-3-Clause",
  "author": "The Caleydo Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jku-icg/jupyterlab_nbprovenance.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "extension:install": "jupyter labextension install jupyterlab_nbprovenance",
    "extension:uninstall": "jupyter labextension uninstall  jupyterlab_nbprovenance",
    "extension:enable": "jupyter labextension enable jupyterlab_nbprovenance",
    "extension:disable": "jupyter labextension disable jupyterlab_nbprovenance"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.1.0",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/console": "^2.0.2",
    "@jupyterlab/coreutils": "^4.0.2",
    "@jupyterlab/filebrowser": "^2.0.2",
    "@jupyterlab/mainmenu": "^2.0.2",
    "@jupyterlab/nbformat": "^2.0.1",
    "@jupyterlab/notebook": "^2.0.2",
    "@jupyterlab/rendermime-interfaces": "^2.0.1",
    "@lumino/widgets": "^1.11.1",
    "@types/circular-json": "^0.4.0",
    "@types/d3": "^5.7.2",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@visdesignlab/trrack": "^1.2.0",
    "@jku-icg/trrack-notebook-vis": "^1.0.0-alpha.2",
    "awesome-typescript-loader": "^5.2.1",
    "circular-json": "^0.5.9",
    "d3": "^5.12.0",
    "font-awesome": "^4.7.0",
    "http-server": "^0.12.0",
    "node-sass": "^4.13.1",
    "react": "16.9.0",
    "react-anaconda": "^1.0.1",
    "react-dom": "16.9.0",
    "react-hot-loader": "^4.12.21",
    "react-move": "^6.1.0",
    "semantic-ui-react": "^0.88.2",
    "ts-loader": "^6.0.4",
    "typestyle": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "devDependencies": {
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "watch": "^1.0.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
