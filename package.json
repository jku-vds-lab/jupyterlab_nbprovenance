{
  "name": "@jku-icg/jupyterlab_nbprovenance",
  "version": "1.0.0-beta.1",
  "description": "JuypterLab extension for provenance tracking in notebooks.",
  "keywords": [
    "jupyter",
    "notebook",
    "jupyterlab",
    "jupyterlab-extension",
    "provenance",
    "reproducability"
  ],
  "homepage": "https://github.com/jku-vds-lab/jupyterlab_nbprovenance/",
  "bugs": {
    "url": "https://github.com/jku-vds-lab/jupyterlab_nbprovenance/issues"
  },
  "license": "MIT",
  "author": "JKU Visual Data Science Lab",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jku-vds-lab/jupyterlab_nbprovenance.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "extension:install": "jupyter labextension install jupyterlab_nbprovenance",
    "extension:uninstall": "jupyter labextension uninstall  jupyterlab_nbprovenance",
    "extension:enable": "jupyter labextension enable jupyterlab_nbprovenance",
    "extension:disable": "jupyter labextension disable jupyterlab_nbprovenance"
  },
  "dependencies": {
    "@jku-icg/trrack-notebook-vis": "^1.0.0-alpha.5",
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/ui-components": "^3.0.0",
    "@lumino/widgets": "^1.23.0",
    "@types/d3": "^5.7.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@visdesignlab/trrack": "2.0.0-alpha.10",
    "awesome-typescript-loader": "^5.2.1",
    "d3": "^5.12.0",
    "font-awesome": "^4.7.0",
    "sass": "^1.42.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^6.0.4",
    "typestyle": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "devDependencies": {
    "cypress": "8.2.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "watch": "^1.0.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
